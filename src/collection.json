{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "init": {
      "factory": "./init/schematic",
      "schema": "./init/schema.json",
      "description": "Initialize the @opi_pib/ngx-schematics plugin",
      "aliases": ["ng-add"],
      "hidden": true
    },
    "entity": {
      "description": "entity generator",
      "factory": "./entity/index#entity"
    },
    "value-object": {
      "description": "value-object generator",
      "factory": "./value-object/index#valueObject"
    }
  }
}
